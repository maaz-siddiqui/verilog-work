    %store/vec4 v0x55f10cfb2880_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55f10cfb27b0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55f10cfb2880_0, 0, 1;
    %delay 5, 0;
